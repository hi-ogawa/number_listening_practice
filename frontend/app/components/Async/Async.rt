<div>
  <button onClick={this.onRequestButtonClick}> Start Request </button>
  <p rt-if="this.requestState == 'NOT_STARTED'"> not started </p>
  <p rt-if="this.requestState == 'STARTED'"> started </p>
  <div rt-if="this.requestState == 'FINISHED'">
    finished
    <img src={this.resp.data.avatar_url} />
    <this.PreJSON json={this.resp.data}/>
  </div>
  <p rt-if="this.requestState == 'ERROR'"> error </p>
</div>
